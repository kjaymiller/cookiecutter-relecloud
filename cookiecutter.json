{
  "project_name": "Relecloud",
  "project_slug": "demo-code",
  "azd_template_version": "0.0.1",
  "project_backend": ["django", "fastapi", "flask"],
  "use_vnet": "n",
  "db_resource": ["postgres-flexible"],
  "__repo_name": "relecloud-{{cookiecutter.project_backend}}-{{cookiecutter.db_resource}}",
  "__src_folder_name": "{{ cookiecutter.project_slug.lower()|replace(' ', '_')|replace('-', '_')|replace('.', '_')|trim() }}",
  "__project_short_description": "Create a relecloud demo application with {{cookiecutter.project_backend}} and {{cookiecutter.db_resource}}",
  "web_port": "8000",
  "_copy_without_render": [
    "root/.github/workflows/*"
  ],
  "_extensions": ["extensions.GetUrlForBackend"],
  "_jinja2_env_vars": {
      "lstrip_blocks": true,
      "trim_blocks": true
  }
}